###### Classes ###### 
cco:ont00000319 a owl:Class ;
    rdfs:subClassOf obo:BFO_0000031 ;
    rdfs:label "Artifact Design"@en ; 
    skos:definition "A design "@en  .

ex:JetEngine a owl:Class;
    rdfs:subClassOf obo:BFO_0000040 ; 
    rdfs:label "Jet Engine"@en ; 
    skos:definition "a jet engine"@en  .

ex:FixedWing a owl:Class;
    rdfs:subClassOf obo:BFO_0000040 ;
    rdfs:label "Fixed Wing" @en ; 
    skos:definition "An aircraft with a fixed wing" @en .

ex:TestProcess a owl:Class;
    rdfs:subClassOf obo:BFO_0000015 ;
    rdfs:label "Test Process" @en; 
    skos:definition "A process to evaluate a design"@en .

ex:MeasurementUnit a owl:Class;
    rdfs:subClassOf obo:BFO_0000031 ;
    rdfs:label "Measurement Unit" @en ; 
    skos:definition "A unit that is specifed"@en  .

ex:ActofAircraftProcessing a owl:Class;
    rdfs:subClassOf obo:BFO_0000015 ;
    rdfs:label "Act of aircraft processing"@en ; 
    skos:definition "A process to analyzed aircraft artifacts" @en.

ex:AdaptabilityEvaluation a owl:Class;
    rdfs:subClassOf obo:BFO_0000015;
    rdfs:label "Adaptability Evaluation" @en; 
    skos:definition "A process to evaluate adaptability "@en  .

ex:ActofMeasuring a owl:Class;
    rdfs:subClassOf obo:BFO_0000015;
    rdfs:label "Act of Measuring"@en ; 
    skos:definition "A process to obtain a measurement"@en .

ex:TotalEmployment a owl:Class;
    rdfs:subClassOf obo:BFO_0000019;
    rdfs:label "Total Employment"@en ;
    skos:definition "A quality of total employment"@en .

###### Object Properties ###### 
obo:BFO_0000178 rdf:type owl:ObjectProperty;
    rdfs:label "has continuant part" @en; 
    skos:definition "Relates a continuat to one if its continuant parts"@en .

cco:ont00001808 a owl:ObjectProperty ;
    rdfs:label "is About"@en ; 
    skos:definition "Relates to something"@en  .

ex:isTemporalPartof a owl:ObjectProperty; 
    rdfs:label "is Temporal part of"@en; 
    skos:definition "Relates an occurent to a larger occurant which is a temporal part"@en .

cco:ont00000120 a owl:ObjectProperty;
    rdfs:label " is measurement unit of"@en; 
    skos:definition "Relates a unit specification " @en .

cco:ont00001942 a owl:ObjectProperty;
    rdfs:label "prescribes"@en ; 
    skos:definition "Relates a design as a contraint."@en .
cco:ont00001947 a owl:ObjectProperty;
    rdfs:label "quality"@en; 
    skos:definition "A measurement of an entity. "@en . 

ex:hasArea a owl:ObjectProperty; 
    rdfs:label "has Area"@en; 
    skos:definition "Relates to the length times width."@en . 
obo:BFO_0000055 a owl:ObjectProperty;
    rdfs:label "realizes"@en ;
    skos:defintion "realizations"@en .
cco:ont00001816 a owl:ObjectProperty;
    rdfs:label "is output of "@en;
    skos:definition "the outpuf of some process/measurement"@en.

obo:BFO_0000197 a owl:ObjectProperty; 
    rdfs:label "inheres in"@en;
    skos:definition "b inheres in c =Def b is a specifically dependent continuant & c is an independent continuant that is not a spatial region & b specifically depends on c"@en.

ex:hasManufacturer a owl:ObjectProperty ;
  rdfs:label "has manufacturer"@en ;
  skos:definition " its manufacturer."@en .
###### Datatype Properties ###### 
ex:oGroup a owl:DatatypeProperty;
