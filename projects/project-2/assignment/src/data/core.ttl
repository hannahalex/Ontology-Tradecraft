###### Classes ###### 
cco:ont00000319 a owl:Class ;
    rdfs:subClassOf obo:BFO_0000031 ;
    rdfs:label "Artifact Design"; 
    skos:definition "A design " .

ex:JetEngine a owl:Class;
    rdfs:subClassOf obo:BFO_0000040 ; 
    rdfs:label "Jet Engine"; 
    skos:definition "a jet engine" .

ex:FixedWing a owl:Class;
    rdfs:subClassOf obo:BFO_0000040 ;
    rdfs:label "Fixed Wing"; 
    skos:definition "An aircraft with a fixed wing" .

ex:TestProcess a owl:Class;
    rdfs:subClassOf obo:BFO_0000015 ;
    rdfs:label "Test Process"; 
    skos:definition "A process to evaluate a design" .

ex:MeasurementUnit a owl:Class;
    rdfs:subClassOf obo:BFO_0000031 ;
    rdfs:label "Measurement Unit"; 
    skos:definition "A unit that is specifed"  .

ex:ActofAircraftProcessing a owl:Class;
    rdfs:subClassOf obo:BFO_0000015 ;
    rdfs:label "Act of aircraft processing"; 
    skos:definition "A process to analyzed aircraft artifacts" .

ex:AdaptabilityEvaluation a owl:Class;
    rdfs:subClassOf obo:BFO_0000015;
    rdfs:label "Adaptability Evaluation"; 
    skos:definition "A process to evaluate adaptability "  .

ex:ActofMeasuring a owl:Class;
    rdfs:subClassOf obo:BFO_0000015;
    rdfs:label "Act of Measuring"; 
    skos:definition "A process to obtain a measurement" .

ex:TotalEmployment a owl:Class;
    rdfs:subClassOf obo:BFO_0000019;
    rdfs:label "Total Employment" ;
    skos:definition "A quality of total employment" .

###### Object Properties ###### 
obo:BFO_0000178 rdf:type owl:ObjectProperty;
    rdfs:label "has continuant part"; 
    skos:definition "Relates a continuat to one if its continuant parts" .

cco:ont00001808 a owl:ObjectProperty ;
    rdfs:label "is About"; 
    skos:definition "Relates to something" .

ex:isTemporalPartof a owl:ObjectProperty; 
    rdfs:label "is Temporal part of"; 
    skos:definition "Relates an occurent to a larger occurant which is a temporal part" .

cco:ont00000120 a owl:ObjectProperty;
    rdfs:label " is measurement unit of"; 
    skos:definition "Relates a unit specification "  .

cco:ont00001942 a owl:ObjectProperty;
    rdfs:label "prescribes"; 
    skos:definition "Relates a design as a contraint." .
cco:ont00001947 a owl:ObjectProperty;
    rdfs:label "quality"; 
    skos:definition "A measurement of an entity. " . 

ex:hasArea a owl:ObjectProperty; 
    rdfs:label "has Area"; 
    skos:definition "Relates to the length times width." . 
obo:BFO_0000055 a owl:ObjectProperty;
    rdfs:label "realizes";
    skos:defintion "realizations".
cco:ont00001816 a owl:ObjectProperty;
    rdfs:label "is output of ";
    skos:definition "the outpuf of some process/measurement".

obo:BFO_0000197 a owl:ObjectProperty; 
    rdfs:label "inheres in";
    skos:definition "b inheres in c =Def b is a specifically dependent continuant & c is an independent continuant that is not a spatial region & b specifically depends on c".
    
###### Datatype Properties ###### 
ex:oGroup a owl:DatatypeProperty;
    rdfs:label "O Group"; 
    skos:definition "data headers string" . 

ex:ownCode a owl:DatatypeProperty;
    rdfs:label "own Code" ;
    skos:definition "A string of instruction" . 

ex:hasModelBADA a owl:DatatypeProperty;
    rdfs:range xsd:string; 
    rdfs:label "has Model BADA";
    skos:definition "a BADA model" . 

 ex:hasModelFAA a owl:DatatypeProperty;
    rdfs:range xsd:decimal; 
    rdfs:label "has Model FAA";
    skos:definition "a FAA model" . 

ex:hasLengthFT a owl:DatatypeProperty;
    rdfs:range xsd:decimal; 
    rdfs:label "has length FT";
    skos:definition "The overall lenght in feet " . 

ex:hasTailHeightFt a owl:DatatypeProperty ;
  rdfs:label "has Tail HeightFt"@en ;
