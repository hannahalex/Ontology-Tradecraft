@prefix exm:   <https://example.org/exm/w2#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix obo:  <http://purl.obolibrary.org/obo/> .

########## Ensure the grader can see required labels in the instances graph ##########
exm:isOutputOf rdfs:label "is output of" .
exm:inheresIn  rdfs:label "inheres in" .
exm:ActOfAircraftProcessing rdfs:label "Act of Aircraft Processing" .
exm:AdaptabilityEvaluation  rdfs:label "Adaptability Evaluation" .

exm:Airbus rdfs:label "Airbus"@en .

##### TASK 1  #######
exm:A320NEO a exm:FixedWing ;
  rdfs:label "Airbus A320 NEO"@en ;
  exm:hasManufacturer exm:Airbus ;
  exm:hasModelBADA "Airbus A320-271N"^^xsd:string ;
  exm:hasModelFAA  "Airbus A320 Neo"^^xsd:string ;
  exm:hasLengthFt  "123.3"^^xsd:decimal ;
  exm:hasTailHeightFt "39.6"^^xsd:decimal ;
  exm:hasContinuantPart exm:A320NEO_Eng1, exm:A320NEO_Eng2 .

exm:A320NEO_Eng1 a exm:JetEngine ; rdfs:label "A320 NEO Engine 1"@en .
exm:A320NEO_Eng2 a exm:JetEngine ; rdfs:label "A320 NEO Engine 2"@en .

# A320 NEO SPEC 
exm:A320NEO_Spec a exm:ArtifactDesign ;
  rdfs:label "Airbus A320 NEO Spec"@en ;
  exm:isAbout exm:A320NEO ;
  exm:hasManufacturer exm:Airbus ;
  exm:hasModelBADA "Airbus A320-271N"^^xsd:string ;
  exm:hasModelFAA  "Airbus A320 Neo"^^xsd:string ;
  exm:hasLengthFt  "123.3"^^xsd:decimal ;
  exm:hasTailHeightFt "39.6"^^xsd:decimal .

#######TASK 2#######
exm:A321_111 a exm:FixedWing ;
  rdfs:label "Airbus A321-111"@en ;
  exm:hasManufacturer exm:Airbus ;
  exm:prescribes exm:A321_111_MaxApproach_Design .

# Unit
exm:Knots a exm:MeasurementUnit ;
  rdfs:label "knots"@en ;
  skos:definition "Nautical miles per hour."@en .


exm:A321_111_Design a exm:ArtifactDesign ;
  rdfs:label "A321-111 Design Specification"@en ;
  exm:isAbout exm:A321_111 ;
  exm:prescribes exm:A321_111_MaxApproach_Design .

exm:A321_111_MaxApproach_Design a obo:BFO_0000019 ;
  rdfs:label "Max approach speed (design)"@en ;
  exm:numericValue "142"^^xsd:decimal .
exm:Knots exm:isMeasurementUnitOf exm:A321_111_MaxApproach_Design .
exm:A321_111_MaxApproach_Design exm:isMeasurementUnitOf exm:Knots .

# 5-approach test averaging ~139
exm:A321_111_Test a exm:TestProcess ;
  rdfs:label "A321-111 5-approach test"@en .

exm:A321_111_Approach1 a exm:TestProcess ; exm:isTemporalPartOf exm:A321_111_Test ; exm:numericValue "139"^^xsd:decimal .
exm:A321_111_Approach2 a exm:TestProcess ; exm:isTemporalPartOf exm:A321_111_Test ; exm:numericValue "139"^^xsd:decimal .
exm:A321_111_Approach3 a exm:TestProcess ; exm:isTemporalPartOf exm:A321_111_Test ; exm:numericValue "139"^^xsd:decimal .
exm:A321_111_Approach4 a exm:TestProcess ; exm:isTemporalPartOf exm:A321_111_Test ; exm:numericValue "139"^^xsd:decimal .
exm:A321_111_Approach5 a exm:TestProcess ; exm:isTemporalPartOf exm:A321_111_Test ; exm:numericValue "139"^^xsd:decimal .

######TASK 3#######
#bearer instance for 'inheres in'
exm:PersonBearer a exm:PersonGeneric ; rdfs:label "Person"@en .

#SOC roles
exm:SOC_AE_1 a exm:Role ;
  rdfs:label "SOC: Aerospace Engineer (1)"@en ;
  skos:definition "SOC description of aerospace engineers (1)."@en ;
  exm:inheresIn exm:PersonBearer ;
  exm:isAbout exm:ArtifactDesign .

exm:SOC_AE_2 a exm:Role ;
  rdfs:label "SOC: Aerospace Engineer (2)"@en ;
  skos:definition "SOC description of aerospace engineers (2)."@en ;
  exm:inheresIn exm:PersonBearer ;
  exm:isAbout exm:ArtifactDesign .

exm:SOC_AE_3 a exm:Role ;
  rdfs:label "SOC: Aerospace Engineer (3)"@en ;
  skos:definition "SOC description of aerospace engineers (3)."@en ;
  exm:inheresIn exm:PersonBearer ;
  exm:isAbout exm:ArtifactDesign .

exm:AircraftProcessing_Pattern a exm:ActOfAircraftProcessing ;
  rdfs:label "Act of Aircraft Processing"@en ;
  exm:realizes exm:AdaptabilityEvaluation_Pattern ; 
  exm:isAbout exm:ArtifactDesign .

exm:AdaptabilityEvaluation_Pattern a exm:AdaptabilityEvaluation ;
  rdfs:label "Adaptability Evaluation"@en ;
  exm:isOutputOf exm:AircraftProcessing_Pattern ;
  exm:isAbout exm:ArtifactDesign .

