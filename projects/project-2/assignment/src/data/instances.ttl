####### TASK 1 #######
    ex:Airbus rdfs:label "Airbus" .
#Class Fixed Wing
    ex:A320NEO a ex:FixedWing ;
        rdfs:label "Airbus A320 NEO";
        ex:hasContinuantPart ex:A320NEO_Engine1, ex:A320NEO_Engine2 ;
        ex:hasManufacturer ex:Airbus; 
        ex:hasLength "123.3"^^xsd:decimal  ;
        ex:hasTailHeightFt "39.6"^^xsd:decimal ;
        ex:hasModelFAA "Airbus A320"^^xsd:string ;
        ex:hasModelBADA "Airbus A320-231"^^xsd:string .       

    #Class A320NEO_Spec
    ex:A320NEO_Spec a ex:ArtifactDesign ;
        rdfs:label "Airbus A320 NEO Spec"; 
        ex:isAbout ex:A320NEO;
        ex:hasManufacturer ex:Airbus; 
        ex:hasLength "123.3"^^xsd:decimal  ;
        ex:hasTailHeightFt "39.6"^^xsd:decimal ;
        ex:hasModelFAA "Airbus A320"^^xsd:string ;
        ex:hasModelBADA "Airbus A320-231"^^xsd:string .  

    ex:A320NEO_Engine1 a ex:JetEngine; rdfs:label "A320 NEO Engine 1" .
    ex:A320NEO_Engine2  a ex:JetEngine ; rdfs:label "A320 NEO Eninge 2" .

    
####### TASK 2 #######
    #A321_111 is a Fixedwing??
    ex:A321-111 a ex:FixedWing ;
        rdfs:label "Airbus A321-111" ;
        ex:prescribes ex:A321_111_MaxApproachDesign ;
        ex:hasManufactuer ex:Airbus.
    #why can't i change these 2 around?
    
    #it has a design
    ex:A3211_111_design a ex::ArtifactDesign; 
        rdfs:label "A321_111 design";
        ex:isAbout ex:A321-111;
        ex:prescribes ex:A321_111_MaxApproachDesign ;
        ex:prescribe ex:knots .

    ex:knots a ex:MeasurementUnit ;
        rdfs:label "knots";
        skos:defintion "one nautical mile per hour" ;
        ex:ex:isMeasurementUnitof ex:A321_111_MaxApproachDesign;
        ex:numericValue "142"^^xsd:decimal . 

    #the design is a quality, that has
    ex:A321_111_MaxApproachDesign a obo:BFO_000019 ;
        rdfs:label "Max approach speed";
        ex:numericValue "142"^^xsd:decimal . 

    #there are tests that are run for A321_111 with values
    ex:A321_111_Testing a ex:TestProcess;
        rdfs:lable "The five test of A321_111" .

    ex:A321_111_1 a ex:TestProcess; 
        ex:isTemporalPartof ex:A321_111_Testing;
        ex:numericValue "139"^^xsd:int . 
    ex:A321_111_2 a ex:TestProcess; 
        ex:isTemporalPartof ex:A321_111_Testing;
        ex:numericValue "139"^^xsd:int . 
    ex:A321_111_3 a ex:TestProcess; 
       ex:isTemporalPartof ex:A321_111_Testing;
        ex:numericValue "139"^^xsd:int . 
    ex:A321_111_4 a ex:TestProcess; 
       ex:isTemporalPartof ex:A321_111_Testing ;
        ex:numericValue "139"^^xsd:int. 
    ex:A321_111_5 a ex:TestProcess ;
       ex:isTemporalPartof ex:A321_111_Testing;
        ex:numericValue "139"^^xsd:int.  

